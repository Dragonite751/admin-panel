{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": 0,
    "react/display-name": "off",
    "default-param-last": "off",
    "react/react-in-jsx-scope": "off"
  },
  "settings": { "import/resolver": { "node": { "paths": ["src"] } } }
}
